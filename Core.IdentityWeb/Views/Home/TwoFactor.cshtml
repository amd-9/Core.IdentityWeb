@model TwoFactorModel

<div class="row">
    <div class="col-md-4">
        <h1 class="text-center">2SV/2FA</h1>
        <p>Please enter your code below</p>
        <form asp-action="TwoFactor" method="post" class="form-horizontal">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="Token" class="control-label"></label>
                <input asp-for="Token" class="form-control" />
                <span asp-validation-for="Token" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Submit" class="btn btn-default" />
                <div asp-validation-summary="All"></div>
            </div>
        </form>
    </div>
</div>
